#include <iostream>
using namespace std;
int main() {
	int m, i, j,p,s=0,a;
	cin >> m;
	if (m <= 0) 
		cout << "Данные введены некорректно";
	else if (m == 1) {
		cin >> a;
		cout << 0;
	}
	else {
		int** v = (int**)malloc(m * sizeof(int*));
		for (int i = 0; i < m; i++)
			v[i] = (int*)malloc(m * sizeof(int));
		for (i = 0; i < m; i++)
			for (j = 0; j < m; j++)
				cin >> v[i][j];
		j = m - 2;
		a = j;
		cout << v[0][m - 1] << " ";
		while (a!=0) {
			i = 0;
			j = a;
			while (j != m) {
				s = s + v[i][j];
				i++;
				j++;
		}
			cout << s << " ";
			a--;
			s = 0;
		}
		a = 1;
		p = 1;
		while (a != (2 * m - 2) / 2) {
			j = 0;
			i = p;
			while (i != m) {
				s = s + v[i][j];
				i++;
				j++;
			}
			cout << s << " ";
			a++;
			s = 0;
			p++;
		}
		cout << v[m-1][0];
		free(v);
	}
	return 0;
}
